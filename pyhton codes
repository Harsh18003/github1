my_points = {'a':(4,3),'D':(1,2),'c':(5,1)}
highest = [0,0]
init = 0
for a in range(2):
    init = 0
    for b in my_point.keys():
        val = my_point[b][a]
        if init == 0:
            highest[a]=val
        init+=1
        if val>highest[a]:
            highest[a]=val
    print("Maximum value at index (my_point) = ",highest[a])
